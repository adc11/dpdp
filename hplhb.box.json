{
    "id": "ziye.hplhbapp.sub",
    "name": "ziye 哈皮领红包订阅",
    "author": "@ZIYE",
    "icon": "https://cdn.jsdelivr.net/gh/ziye888/JavaScript@main/ziye.png",
    "repo": "https://github.com/ziye888",
    "apps": [{
        "id": "hplhbappsss",
        "name": "哈皮领红包",
        "keys": "keys": ["hplhbbody", "hplhbbody2", "hplhbbody3", "hplhbbody4", "hplhbbody5", "hplhbbody6", "hplhbbody7", "hplhbbody8", "hplhbbody9", "hplhbbody10", "hplhbbody11", "hplhbbody12", "hplhbbody13", "hplhbbody14", "hplhbbody15", "hplhbbody16", "hplhbbody17", "hplhbbody18", "hplhbbody19", "hplhbbody20"],
        "author": "@ziye",
        "settings": [{
            "id": "hplhbSuffix",
            "name": "当前账号",
            "val": "1",
            "type": "number",
            "desc": "当前抓取ck记录的账号序号，如：1、2、3、"
        }, {
            "id": "hplhbCount",
            "name": "账号个数",
            "val": "1",
            "type": "number",
            "desc": "指定任务最多跑几个账号，根据抓取的账号数据个数来设值"
        }, {
            "id": "hplhbXH",
            "name": "循环获取CK",
            "val": "0",
            "type": "number",
            "desc": "0关闭 1开启,默认关闭"
        }, {
            "id": "hplhbTXTX",
            "name": "提现提醒",
            "val": "0",
            "type": "number",
            "desc": "0关闭 设置余额大于多少提醒"
        }, {
            "id": "hplhbXYZ",
            "name": "执行概率",
            "val": "100",
            "type": "number",
            "desc": "0不执行 可设置0-100,默认百分百"
        }, {
            "id": "hplhbnotifyttt",
            "name": "推送控制",
            "val": "1",
            "type": "number",
            "desc": "0关闭，1推送,默认12点以及23点推送"
        }, {
            "id": "hplhbnotifyInterval",
            "name": "通知控制",
            "val": "2",
            "type": "number",
            "desc": "0关闭，1为 所有通知，2为 12，23 点通知，3为 6，12，18，23 点通知 "
        }, {
            "id": "hplhbMinutes",
            "name": "推送-通知 分钟控制",
            "val": "10",
            "type": "number",
            "desc": "推送以及通知控制在什么分钟段，可设置0-59,默认0到10"
        }, {
        "id": "hplhbID",
        "name": "ID列表",
        "val": "",
        "type": "textarea",
        "rows": 8,
        "desc": "红包链接 列表"
    }],
        "repo": "https://cdn.jsdelivr.net/gh/ziye888/JavaScript@main/Task/hplhb.js",
        "icons": ["https://cdn.jsdelivr.net/gh/ziye888/JavaScript@main/Task/hplhb.png", "https://cdn.jsdelivr.net/gh/ziye888/JavaScript@main/Task/hplhb.png"],
        "script": "https://cdn.jsdelivr.net/gh/ziye888/JavaScript@main/Task/hplhb.js"
    }]
}
